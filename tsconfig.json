{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es5", "es2015", "es2016", "es2017", "DOM"],
    "jsx": "react",
    "strict": true,
    "noImplicitAny": true,
    "baseUrl": "./",
    "paths": {
      "~*": ["./src/*"]
    },
    "typeRoots": ["./node_modules/@types", "./typings"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
